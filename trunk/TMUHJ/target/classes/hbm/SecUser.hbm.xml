<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<union-subclass name="j.index.module.security.entity.SecUser"
		table="accountNumber" extends="j.index.core.entity.GenericEntity">

		<property name="userId" column="userID" length="20" type="string"
			not-null="true" unique="true" />

		<property name="userPw" column="userPW" length="200" type="string"
			not-null="true" />

		<property name="userName" column="userName" length="50" type="string"
			not-null="true" />

		<property name="role" column="role" length="50" type="string"
			not-null="true" />

		<property name="email" column="email" length="50" type="string" />

		<property name="status" column="status" length="50" type="string" />

		<property name="tel" column="TEL" length="10" type="integer" />

		<property name="stopDate" column="STOP_DATE"
			type="org.jadira.usertype.dateandtime.joda.PersistentLocalDateTime" />

	</union-subclass>

</hibernate-mapping>