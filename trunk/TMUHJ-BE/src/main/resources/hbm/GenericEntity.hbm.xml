<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="j.index.core.entity.GenericEntity" abstract="true">

		<id name="serNo" column="serNo" type="long">
			<generator class="increment" />
		</id>

		<property name="cUid" column="cUid" length="10" type="string"
			not-null="true" />

		<property name="uUid" column="uUid" length="10" type="string"
			not-null="true" />

		<property name="cDTime" column="cDTime" not-null="true"
			type="org.jadira.usertype.dateandtime.joda.PersistentLocalDateTime" />

		<property name="uDTime" column="uDTime" not-null="true"
			type="org.jadira.usertype.dateandtime.joda.PersistentLocalDateTime" />

	</class>

</hibernate-mapping>